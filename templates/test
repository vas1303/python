using System;
class Program
{
    static void Main(string[] args)
    {
        int a;
        Console.WriteLine($"Введите число");
         a = Convert.ToInt32(Console.ReadLine());
        int max = a;
        for (int i =1 ; i < 10; i++)
        {
            Console.WriteLine($"введите число");
            a = Convert.ToInt32(Console.ReadLine());
            if (a > max) max = a;
        }
        Console.WriteLine($"максимум {max}");
    }
}
